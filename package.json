{
  "name": "totl-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n css,web \"npm run tailwind:dev\" \"vite\"",
    "tailwind:dev": "tailwindcss -i ./src/index.css -o ./src/output.css --watch",
    "tailwind:build": "tailwindcss -i ./src/index.css -o ./src/output.css --minify",
    "build": "npm run tailwind:build && tsc -b && vite build",
    "check": "npm run tailwind:build && tsc -b && vite build",
    "check:build": "./scripts/check-build.sh",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.75.0",
    "browser-image-compression": "^2.0.2",
    "canvas-confetti": "^1.9.4",
    "csv-parse": "^6.1.0",
    "despia-native": "^1.0.14",
    "dotenv": "^17.2.3",
    "emoji-picker-react": "^4.16.1",
    "html-to-image": "^1.11.13",
    "html2canvas": "^1.4.1",
    "react": "18.3.1",
    "react-chat-elements": "^12.0.18",
    "react-confetti": "^6.4.0",
    "react-dom": "18.3.1",
    "react-easy-crop": "^5.5.6",
    "react-router-dom": "^7.9.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.36.0",
    "@netlify/functions": "^5.0.1",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/addon-themes": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@types/canvas-confetti": "^1.9.0",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^10.0.8",
    "globals": "^16.4.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "storybook": "^10.0.8",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.14"
  }
}
